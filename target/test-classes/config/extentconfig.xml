<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>

        <!-- report theme -->
        <!-- STANDARD, DARK -->
        <theme>STANDARD</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>utf-16</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <!-- HTTP, HTTPS -->
        <protocol>HTTP</protocol>

        <!-- offline report -->
        <timelineEnabled>true</timelineEnabled>

        <!-- offline report -->
        <offlineMode>true</offlineMode>

        <thumbnailForBase64>false</thumbnailForBase64>

        <!-- title of the document -->
        <documentTitle>Test Automation Results</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>AutomationReport</reportName>

        <!-- timestamp format -->
        <timeStampFormat>MMM dd, yyyy HH:mm:ss a</timeStampFormat>

        <!-- custom javascript -->
        <js>js1</js>

        <!-- custom styles -->
        <css>css1</css>

    </configuration>
</extentreports>